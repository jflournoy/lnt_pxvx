USEVARIABLES =
aI_A
bI_A
cI_A
dI_A
aMVI_POMP
bMVI_POMP
cMVI_POMP
dMVI_POMP
a1
a2
a3
a4
;

missing = all (-9999);

USEOBSERVATIONS are Sample == 1; ! National Sample

tscores = a1 a2 a3 a4;

DEFINE:
a1 = aAGE + 0 - 36;
a2 = aAGE + 1 - 36;
a3 = aAGE + 2 - 36;
a4 = aAGE + 3 - 36;

ANALYSIS:
TYPE=RANDOM;
PROCESSORS = 4; 
model=nocovariances; ! supresses default estimation of certain covs in model;

MODEL:

i_I_A s_I_A | aI_A bI_A cI_A dI_A AT a1 a2 a3 a4;
i_MVI_POMP s_MVI_POMP | aMVI_POMP bMVI_POMP cMVI_POMP dMVI_POMP AT a1 a2 a3 a4;

! freeing factor covs because of "nocovariances"
s_I_A WITH i_I_A s_MVI_POMP i_MVI_POMP; 
i_I_A WITH s_MVI_POMP i_MVI_POMP;
s_MVI_POMP WITH i_MVI_POMP;

p1 BY aI_A@1; ! Residual "factors";
p2 BY bI_A@1;
p3 BY cI_A@1;
p4 BY dI_A@1;

v1 BY aMVI_POMP@1; ! Residual "factors";
v2 BY bMVI_POMP@1;
v3 BY cMVI_POMP@1;
v4 BY dMVI_POMP@1;

!residual variances on observed/manifest vars set to 0
aI_A@0;
bI_A@0;
cI_A@0;
dI_A@0;

!residual variances on observed/manifest vars set to 0
aMVI_POMP@0;
bMVI_POMP@0;
cMVI_POMP@0;
dMVI_POMP@0;

! means of observed/manifest vars set to 0
[aI_A@0];
[bI_A@0];
[cI_A@0];
[dI_A@0];

! means of observed/manifest vars set to 0
[aMVI_POMP@0];
[bMVI_POMP@0];
[cMVI_POMP@0];
[dMVI_POMP@0];

[p1@0]; ! means of residual "factors" set to 0
[p2@0];
[p3@0];
[p4@0];

[v1@0]; ! means of residual "factors" set to 0
[v2@0];
[v3@0];
[v4@0];

p4 on p3; 
p3 on p2;
p2 on p1;

v4 on v3; 
v3 on v2;
v2 on v1;

p4 on v3;
p3 on v2;
p2 on v1;

v4 on p3;
v3 on p2;
v2 on p1;

p1 with v1;
p2 with v2;
p3 with v3;
p4 with v4;

p2 p3 p4; ! estimate variances of residual "factors"
v2 v3 v4; ! estimate variances of residual "factors"

OUTPUT: SAMPSTAT PATTERNS;
