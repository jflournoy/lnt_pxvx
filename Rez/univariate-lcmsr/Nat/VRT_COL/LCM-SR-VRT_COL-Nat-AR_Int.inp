USEVARIABLES =
aVRT_COL
bVRT_COL
cVRT_COL
dVRT_COL
a1
a2
a3
a4
;

missing = all (-9999);

USEOBSERVATIONS are Sample == 1; ! National Sample




tscores = a1 a2 a3 a4;

DEFINE:
a1 = aAGE + 0 - 36;
a2 = aAGE + 1 - 36;
a3 = aAGE + 2 - 36;
a4 = aAGE + 3 - 36;


ANALYSIS:
TYPE=RANDOM;
PROCESSORS = 4; 
model=nocovariances; ! supresses default estimation of certain covs in model;

MODEL:

i_VRT_COL | aVRT_COL bVRT_COL cVRT_COL dVRT_COL AT a1 a2 a3 a4; !Intercept only;




p1 BY aVRT_COL@1; ! Residual "factors";
p2 BY bVRT_COL@1;
p3 BY cVRT_COL@1;
p4 BY dVRT_COL@1;

!residual variances on observed/manifest vars set to 0
aVRT_COL@0;
bVRT_COL@0;
cVRT_COL@0;
dVRT_COL@0;

! means of observed/manifest vars set to 0
[aVRT_COL@0];
[bVRT_COL@0];
[cVRT_COL@0];
[dVRT_COL@0];

p2 p3 p4 (c); ! estimate variances of residual "factors"

[p1@0]; ! means of residual "factors" set to 0
[p2@0];
[p3@0];
[p4@0];

p4 on p3 (a); ! Autoregressive paths constrained to equality
p3 on p2 (a);
p2 on p1 (a);


OUTPUT: SAMPSTAT PATTERNS;
