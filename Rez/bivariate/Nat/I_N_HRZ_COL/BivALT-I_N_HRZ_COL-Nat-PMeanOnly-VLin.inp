USEVARIABLES =
aI_N
bI_N
cI_N
dI_N
aHRZ_COL
bHRZ_COL
cHRZ_COL
dHRZ_COL
bage_c
cage_c
dage_c
;

missing = all (-9999);

USEOBSERVATIONS are Sample == 1; ! National Sample

tscores = bage_c cage_c dage_c;

DEFINE:
bage_c = aAGE + 1 - 36;
cage_c = aAGE + 2 - 36;
dage_c = aAGE + 3 - 36;

ANALYSIS:
TYPE=RANDOM;
PROCESSORS = 8; 

MODEL:

i_I_N s_I_N | bI_N cI_N dI_N AT bage_c cage_c dage_c;
i_HRZ_COL s_HRZ_COL | bHRZ_COL cHRZ_COL dHRZ_COL AT bage_c cage_c dage_c;


!After Bollen and Curran initial observation is not part of LGM

dI_N on cI_N (a); ! Autoregressive paths constrained to equality
cI_N on bI_N (a);
bI_N on aI_N (a);

dHRZ_COL on cHRZ_COL (b); ! Autoregressive paths constrained to equality
cHRZ_COL on bHRZ_COL (b);
bHRZ_COL on aHRZ_COL (b);

bI_N cI_N dI_N (c); !Equality constraint on residuals
bHRZ_COL cHRZ_COL dHRZ_COL (d); !Equality constraint on residuals

dHRZ_COL on cI_N (x); ! Cross-lagged paths constrained to equality
cHRZ_COL on bI_N (x);
bHRZ_COL on aI_N (x);

dI_N on cHRZ_COL (y); ! Cross-lagged paths constrained to equality
cI_N on bHRZ_COL (y);
bI_N on aHRZ_COL (y);

dHRZ_COL with dI_N (w); ! within-wave correlations constrained to equality
cHRZ_COL with cI_N (w);
bHRZ_COL with bI_N (w);

aI_N aHRZ_COL i_I_N i_HRZ_COL 
s_HRZ_COL
with
s_HRZ_COL
aI_N aHRZ_COL i_I_N i_HRZ_COL;

s_I_N@0;


OUTPUT: SAMPSTAT PATTERNS;
