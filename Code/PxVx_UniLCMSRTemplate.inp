USEVARIABLES =
a[[var#which]]
b[[var#which]]
c[[var#which]]
d[[var#which]]
a1
a2
a3
a4
;

missing = all (-9999);

[[which < 27]]
USEOBSERVATIONS are Sample == 1; ! National Sample
[[/which < 27]]

[[which > 26]]
[[which < 53]]
USEOBSERVATIONS are Sample == 2; ! College Sample
[[/which < 53]]
[[/which > 26]]

[[which > 52]]
USEOBSERVATIONS are Sample == 1; ! National Sample, informant's data
[[/which > 52]]


tscores = a1 a2 a3 a4;

DEFINE:
a1 = aAGE + 0 - [[subsetMeanAge#which]];
a2 = aAGE + 1 - [[subsetMeanAge#which]];
a3 = aAGE + 2 - [[subsetMeanAge#which]];
a4 = aAGE + 3 - [[subsetMeanAge#which]];


ANALYSIS:
TYPE=RANDOM;
PROCESSORS = 4; 
model=nocovariances; ! supresses default estimation of certain covs in model;

MODEL:

[[growthtype == 1]]
i_[[var#which]] | a[[var#which]] b[[var#which]] c[[var#which]] d[[var#which]] AT a1 a2 a3 a4; !Intercept only;
[[/growthtype == 1]]

[[growthtype > 1]]
i_[[var#which]] s_[[var#which]] |  a[[var#which]] b[[var#which]] c[[var#which]] d[[var#which]] AT a1 a2 a3 a4;
[[/growthtype > 1]]

[[growthtype == 2]]
s_[[var#which]]@0; ! fix slope covar to 0
[[/growthtype == 2]]

[[growthtype > 2]]
s_[[var#which]] WITH i_[[var#which]]; ! freeing factor covs because of "nocovariances";
[[/growthtype > 2]]

p1 BY a[[var#which]]@1; ! Residual "factors";
p2 BY b[[var#which]]@1;
p3 BY c[[var#which]]@1;
p4 BY d[[var#which]]@1;

!residual variances on observed/manifest vars set to 0
a[[var#which]]@0;
b[[var#which]]@0;
c[[var#which]]@0;
d[[var#which]]@0;

! means of observed/manifest vars set to 0
[a[[var#which]]@0];
[b[[var#which]]@0];
[c[[var#which]]@0];
[d[[var#which]]@0];

p2 p3 p4 (c); ! estimate variances of residual "factors"

[p1@0]; ! means of residual "factors" set to 0
[p2@0];
[p3@0];
[p4@0];

p4 on p3 (a); ! Autoregressive paths constrained to equality
p3 on p2 (a);
p2 on p1 (a);


OUTPUT: SAMPSTAT PATTERNS;
