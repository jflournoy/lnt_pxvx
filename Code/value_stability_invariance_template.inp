USEVARIABLES =
a[[var#which]]
b[[var#which]]
c[[var#which]]
d[[var#which]]
aAGE
;

missing = all (-9999);

[[subsetnum == 1]]
USEOBSERVATIONS are Sample == 1; ! National Sample
[[/subsetnum == 1]]

[[subsetnum == 2]]
USEOBSERVATIONS are Sample == 2; ! College Sample
[[/subsetnum == 2]]

ANALYSIS:
TYPE=RANDOM;
PROCESSORS = 8; 

MODEL:

[[growthtype == 1]]
i_[[var#which]] | b[[var#which]] c[[var#which]] d[[var#which]] AT bage_c cage_c dage_c;
[[/growthtype == 1]]

[[growthtype > 1]]
i_[[var#which]] s_[[var#which]] | b[[var#which]] c[[var#which]] d[[var#which]] AT bage_c cage_c dage_c;
[[/growthtype > 1]]

!After Bollen and Curran initial observation is not part of LGM

d[[var#which]] on c[[var#which]] (a); ! Autoregressive paths constrained to equality
c[[var#which]] on b[[var#which]] (a);
b[[var#which]] on a[[var#which]] (a);

b[[var#which]] c[[var#which]] d[[var#which]] (c); !Equality constraint on residuals

a[[var#which]] i_[[var#which]]
WITH
a[[var#which]] i_[[var#which]];

[[growthtype == 2]]
s_[[var#which]]@0;
[[/growthtype == 2]]

[[growthtype > 2]]
s_[[var#which]] WITH i_[[var#which]] a[[var#which]];
[[/growthtype > 2]]

OUTPUT: SAMPSTAT PATTERNS;
